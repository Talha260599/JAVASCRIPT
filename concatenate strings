
// Using the + operator:

var firstName,lastName,fullName;

firstName= "Nabila Binte Tariq";

lastName= " Mumu ";

fullName= firstName + lastName; 

document.write ("Yes, " + fullName + "is my name");


//output : Yes, Nabila Binte Tariq Mumu is my name





//Using the concat method


var firstName,lastName,fullName;

firstName= "Sonia Tasmin";

lastName= " Sneha ";

fullName= firstName.concat(lastName); 

document.write ("Yes, " + fullName + "is my name <br/>");
document.write (firstName.length);

//output : Yes, Sonia Tasmin Sneha is my name
           12
